[xfgiven_playlistm]
<!-- item__track -->
 <div class="item__track track" data-track="[xfvalue_playlistm]" data-artist="[xfvalue_artist]" data-pic="[xfvalue_pic]" data-title="{title}">
 	<!-- item__track-play -->
	<div class="item__track-play js__play-m" title="Играть \ остановить">
		<span class="md__icon md-plays"></span>
	</div>
	<!-- end item__track-play -->
	<!-- item__track-desc -->
	<a class="item__track-desc" href="{full-link}">
		<div class="item__track-title">{title}</div>
		<div class="item__track-subtitle">[xfvalue_artist]</div>
	</a>
	<!-- end item__track-desc -->
 </div>
<!-- end item__track -->
[/xfgiven_playlistm]

[xfgiven_playlistmpl]
<!-- item__track -->
 <div class="item__track track__pl" data-tracks="[xfvalue_playlistmpl]">
 	<!-- item__track-play -->
	<div class="item__track-play js__play-mpl" title="Играть \ остановить">
		<span class="md__icon md-plays"></span>
	</div>
	<!-- end item__track-play -->
	<!-- item__track-desc -->
	<a class="item__track-desc" href="{full-link}">
		<div class="item__track-title">{title}</div>
		<div class="item__track-subtitle">[xfvalue_artist]</div>
	</a>
	<!-- end item__track-desc -->
 </div>
 <!-- end item__track -->
[/xfgiven_playlistmpl]














